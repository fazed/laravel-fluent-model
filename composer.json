{
  "name": "fazed/laravel-fluent-model",
  "description": "Laravel's model convenience without the Eloquent backend.",
  "keywords": ["fluent", "model", "domain", "laravel"],
  "homepage": "https://github.com/fazed/laravel-fluent-model",
  "license": "MIT",
  "type": "project",
  "authors": [
    {
      "name": "Fazed",
      "email": "thekoetjeable@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2.5",
    "illuminate/contracts": "^6.0|^7.0",
    "illuminate/support": "^6.0|^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0|^9.0",
    "orchestra/testbench": "^4.0|^5.0"
  },
  "autoload": {
    "psr-4": {
      "Fazed\\FluentModel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Fazed\\FluentModel\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Fazed\\FluentModel\\FluentModelServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
